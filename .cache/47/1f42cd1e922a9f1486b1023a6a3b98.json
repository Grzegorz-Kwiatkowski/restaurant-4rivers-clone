55,"column":0},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":124,"column":64}},{"generated":{"line":55,"column":4},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":124,"column":64}},{"generated":{"line":55,"column":8},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":124,"column":68}},{"generated":{"line":55,"column":10},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":124,"column":69}},{"generated":{"line":56,"column":0},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":124,"column":6}},{"generated":{"line":56,"column":4},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":33,"column":4}},{"generated":{"line":56,"column":5},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":32,"column":2}},{"generated":{"line":57,"column":0},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":127,"column":1}},{"name":"GiftCard","generated":{"line":59,"column":15},"source":"../src/Pages/GiftCard/GiftCard.js","original":{"line":129,"column":15}}],"sources":{"../src/Pages/GiftCard/GiftCard.js":"import React, { useState } from \"react\";\nimport { Parallax } from \"react-parallax\";\nimport { Link } from \"react-router-dom\";\nimport { gifts } from '../../data';\n\nimport {\n  GiftCardContainer,\n  GiftCardParallaxWrapper,\n  GiftCardParallaxHeader,\n  GiftCardParallaxContent,\n  GiftCardButton,\n  GiftCardParallaxButtonWrapper,\n  GiftCardParallaxSecondInfo,\n  GiftCardParallaxFirstInfo,\n  GiftCardWrapper,\n  GiftCardHeader,\n  GiftCardInfo,\n  GiftCardArticle,\n  GiftCardText,\n  GiftCardOffers\n} from './GiftCard.elements'\n\nimport ribs from \"../../img/gift-cards/ribs.jpeg\";\nimport Advertisement from '../../components/Advertisement/Advertisement';\nimport Gift from '../../components/Gifts/Gift'\n\n\nfunction GiftCard() {\n\n  const [gift, setGift] = useState(gifts);\n\n  return (\n    <GiftCardContainer>\n\n      <Parallax bgImage={ribs} strength={250}>\n\n        <GiftCardParallaxWrapper>\n\n          <GiftCardParallaxHeader>\n            4 Rivers Gift Cards\n          </GiftCardParallaxHeader>\n\n\n          <GiftCardParallaxContent>\n\n            <GiftCardParallaxFirstInfo>\n              Expertly Smoked BBQ has never been easier to gift to a friend or\n              loved one\n            </GiftCardParallaxFirstInfo>\n\n            <GiftCardParallaxButtonWrapper >\n\n              <GiftCardButton colorBeige >\n                Purchase e-gift card\n              </GiftCardButton>\n\n              <GiftCardButton>\n                Purchase gift card\n              </GiftCardButton>\n\n            </GiftCardParallaxButtonWrapper>\n\n            <GiftCardButton colorBeige>\n              Check your balance\n            </GiftCardButton>\n\n            <GiftCardParallaxSecondInfo>\n              If your card is showing as “Invalid Card Number,” please email us\n              to check your balance.\n            </GiftCardParallaxSecondInfo>\n\n          </GiftCardParallaxContent>\n\n        </GiftCardParallaxWrapper>\n\n      </Parallax>\n\n\n      <GiftCardWrapper>\n\n        <GiftCardHeader>\n          From our Smoker to your Inbox or Mailbox\n        </GiftCardHeader>\n\n        <GiftCardInfo>\n          4R Gift Cards are the perfect gift for any occasion\n        </GiftCardInfo>\n\n        <GiftCardArticle>\n\n          <GiftCardText>\n            Shop now for the quickest way to gift 4 Rivers award-winning BBQ to\n            your friends or loved ones.\n          </GiftCardText>\n\n          <GiftCardText>\n            Our{\" \"}\n            <Link to=\"\" className=\"link\"> 4 Rivers E-Gift Cards  </Link>{\" \"}\n                are fast and easy to order, easily sent to anyone digitally with a\n                few simple clicks directly to their inbox. Additionally, physical{\" \"}\n            <Link to=\"\" className=\"link\">4R Gift Cards</Link>{\" \"}\n            can be purchased online and shipped anywhere in the United States.\n          </GiftCardText>\n\n          <GiftCardText>\n            Shop now for the quickest way to gift 4 Rivers award-winning BBQ to\n            your friends or loved ones.\n          </GiftCardText>\n\n          <GiftCardText>\n            Of course, you can always purchase 4 Rivers Gift Cards at your local\n            Smokehouse. See all the ways to buy below.\n          </GiftCardText>\n\n        </GiftCardArticle>\n\n      </GiftCardWrapper>\n\n\n      <GiftCardOffers>\n        {gift.map(gift => <Gift key={gift.id} image={gift.image} header={gift.header} info={gift.info} btnLabel={gift.btnLabel} link=\"/\" />)}\n      </GiftCardOffers>\n\n      <Advertisement header=\"Hungry Now ?\" label=\"Order online\" link=\"\" />\n    </GiftCardContainer>\n  );\n}\n\nexport default GiftCard;\n"},"lineCount":null}},"error":null,"hash":"23c50ad72c22b95bb36400043ce14b04","cacheData":{"env":{}}}
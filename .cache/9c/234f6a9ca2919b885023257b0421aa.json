s","original":{"line":84,"column":1}},{"name":"CartContent","generated":{"line":46,"column":15},"source":"../src/Pages/Cart/CartContent.js","original":{"line":86,"column":15}}],"sources":{"../src/Pages/Cart/CartContent.js":"import React, { useContext } from 'react'\n\nimport { MainContext } from '../../Contexts/Contexts'\n\nimport {\n    CartContentTrashBin,\n    CartContentImage,\n    CartContentTrashBinWrapper,\n    CartContentLink,\n    CartContentTrashBinMobileWrapper,\n    CartContentTrashBinTableRow\n} from './CartContent.elements';\n\n\nfunction CartContent() {\n\n    const context = useContext(MainContext);\n\n    const fullCartStyle = {\n        display: \"flex\",\n        alignItems: \"center\",\n        justifyContent: \"flex-start\"\n    }\n\n    const addedProducts = context.addedToBasket.map(product => {\n        return (\n            <>\n                <CartContentTrashBinTableRow>\n                    <th>\n                        <CartContentTrashBinMobileWrapper>\n                            <CartContentTrashBin onClick={() => context.deleteFromBasket(product.id)} />\n                        </CartContentTrashBinMobileWrapper>\n                    </th>\n                </CartContentTrashBinTableRow>\n\n                <tr>\n                    <th>Product</th>\n                    <td style={fullCartStyle}>\n                        <CartContentTrashBinWrapper>\n                            <CartContentTrashBin onClick={() => context.deleteFromBasket(product.id)} />\n                        </CartContentTrashBinWrapper>\n\n                        <CartContentImage src={Array.isArray(product.image) ? product.image[0] : product.image} />\n                        <CartContentLink to={`/shop/` + context.changeString(product.header) + '/'}>\n                            {product.header}\n                        </CartContentLink>\n                    </td>\n\n                    <th>Price</th>\n                    <td>\n                        ${product.price}\n                    </td>\n\n                    <th>Quantity</th>\n                    <td>\n                        <input size=\"2\" type=\"number\" value={product.amount} onChange={(e) => context.onChange(product.id, e)} />\n                    </td>\n\n                    <th>Subtotal</th>\n                    <td>\n                        {(product.subtotal).toFixed(2)}\n                    </td>\n                </tr>\n            </>\n\n        )\n    })\n\n\n    return (\n        <>\n            { addedProducts}\n        </>\n\n\n\n    )\n\n\n\n\n\n\n}\n\nexport default CartContent\n"},"lineCount":null}},"error":null,"hash":"49860269c4098d67cf8acbce39775124","cacheData":{"env":{}}}
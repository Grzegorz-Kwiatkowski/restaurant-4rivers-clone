ctions: {\n                    ...openSections,\n                    [label]: !isOpen\n                }\n            });\n        } else {\n            this.setState({\n                openSections: {\n                    [label]: !isOpen\n                }\n            });\n        }\n    };\n\n    render() {\n        const { onClick, props: { children }, state: { openSections } } = this;\n\n        return (\n            <>\n                {children.map(child => (\n                    <AccordionSection\n                        isOpen={!!openSections[child.props.label]}\n                        label={child.props.label}\n                        onClick={onClick}\n                        background={child.props.background}\n                    >\n                        {child.props.children}\n                    </AccordionSection>\n                ))}\n            </>\n        );\n    }\n}\n\nexport default Accordion;"},"lineCount":null}},"error":null,"hash":"c66855f5f0a80838b4b24e301cfb1749","cacheData":{"env":{}}}
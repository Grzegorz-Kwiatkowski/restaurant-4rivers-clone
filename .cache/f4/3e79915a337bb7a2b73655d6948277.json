seContext(MainContext);\n    const [receiptUrl, setReceiptUrl] = useState('')\n\n\n\n    const handleSubmit = async event => {\n        event.preventDefault()\n\n        const { token } = await stripe.createToken({\n            name: 'customer name'\n        })\n\n        const order = await axios.post('http://localhost:7000/api/stripe/charge', {\n            amount: context.total.toString().replace('.', ''),\n            source: token.id,\n            receipt_email: 'customer@example.com'\n        })\n\n        setReceiptUrl(order.data.charge.receipt_url);\n\n    }\n\n    if (receiptUrl) {\n        return (\n            <PaymentSuccesfull url={receiptUrl} />\n        )\n    }\n\n    return (\n        <div>\n            <p>Total : {context.total}</p>\n            <form onSubmit={handleSubmit}>\n                <label>\n                    Card details\n          <CardNumberElement />\n                </label>\n                <br />\n                <label>\n                    Expiration date\n          <CardExpiryElement />\n                </label>\n                <br />\n                <label>\n                    CVC\n          <CardCVCElement />\n                </label>\n                <br />\n                <button type=\"submit\" onClick={() => context.setPaymentSuccess(true)}>\n                    Pay\n        </button>\n\n            </form>\n\n\n\n        </div >\n    )\n\n}\n\nexport default injectStripe(StripeForm)\n"},"lineCount":null}},"error":null,"hash":"1eb882eda9c7fba30c6872213cdc1a76","cacheData":{"env":{}}}
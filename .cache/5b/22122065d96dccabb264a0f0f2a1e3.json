ted":{"line":52,"column":49},"source":"../src/Pages/Shop/Shop.js","original":{"line":90,"column":65}},{"generated":{"line":53,"column":0},"source":"../src/Pages/Shop/Shop.js","original":{"line":83,"column":36}},{"generated":{"line":53,"column":4},"source":"../src/Pages/Shop/Shop.js","original":{"line":83,"column":17}},{"generated":{"line":53,"column":5},"source":"../src/Pages/Shop/Shop.js","original":{"line":82,"column":12}},{"generated":{"line":53,"column":6},"source":"../src/Pages/Shop/Shop.js","original":{"line":33,"column":8}},{"generated":{"line":53,"column":7},"source":"../src/Pages/Shop/Shop.js","original":{"line":31,"column":4}},{"generated":{"line":54,"column":0},"source":"../src/Pages/Shop/Shop.js","original":{"line":95,"column":1}},{"name":"Shop","generated":{"line":56,"column":15},"source":"../src/Pages/Shop/Shop.js","original":{"line":97,"column":15}}],"sources":{"../src/Pages/Shop/Shop.js":"import React, { useContext } from 'react'\nimport { Parallax } from \"react-parallax\";\n\nimport Product from '../../components/Product/Product'\nimport { MainContext } from '../../Contexts/Contexts'\n\nimport {\n    ShopContainer,\n    ShopParallaxWrapper,\n    ShopParallaxHeader,\n    ShopParallaxInfo,\n    ShopWrapper,\n    ShopHeader,\n    ShopProducts\n} from './Shop.elements'\n\nimport shop_banner from '../../img/shop/shop-banner.jpg'\n\n\n\n\n\nfunction Shop() {\n\n    const context = useContext(MainContext);\n\n    const { shopProducts, changeString } = context;\n    const foodAndSauces = shopProducts.filter(product => product.category === \"food&sauces\");\n    const retail = shopProducts.filter(product => product.category === \"retail\");\n\n    return (\n\n        <ShopContainer>\n\n            <Parallax bgImage={shop_banner} strength={250}>\n\n                <ShopParallaxWrapper>\n\n                    <ShopParallaxHeader>\n                        From our family to yours\n                    </ShopParallaxHeader>\n\n                    <ShopParallaxInfo>\n                        Enjoy 4 Rivers Sauces and Foods at Home, at Work and all year Round\n                    </ShopParallaxInfo>\n\n                </ShopParallaxWrapper>\n\n            </Parallax>\n\n\n            <ShopWrapper>\n\n                <ShopHeader>\n                    Food & Sauces\n                </ShopHeader>\n\n            </ShopWrapper>\n\n\n            <ShopProducts>\n                {foodAndSauces.map(item => <Product\n                    key={item.id}\n                    id={item.id}\n                    header={item.header}\n                    price={item.price}\n                    image={Array.isArray(item.image) ? item.image[0] : item.image}\n                    btnLabel=\"view details\"\n                    link={`/shop/` + changeString(item.header) + '/'} />)}\n            </ShopProducts>\n\n\n            <ShopWrapper>\n\n                <ShopHeader>\n                    Retail\n                </ShopHeader>\n\n            </ShopWrapper>\n\n\n            <ShopProducts>\n                {retail.map(item => <Product\n                    key={item.id}\n                    id={item.id}\n                    header={item.header}\n                    price={item.price}\n                    image={Array.isArray(item.image) ? item.image[0] : item.image}\n                    btnLabel=\"view details\"\n                    link={`/shop/` + changeString(item.header) + '/'} />)}\n            </ShopProducts>\n\n        </ShopContainer>\n    )\n}\n\nexport default Shop\n"},"lineCount":null}},"error":null,"hash":"6da8e1ffd698140fad4d282d705c4454","cacheData":{"env":{}}}